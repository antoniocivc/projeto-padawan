pipeline {
     agent any
     stages {
         stage('Build') {
             steps {
                 sh 'echo "Hello World"'
                 sh '''
                     echo "Multiline shell steps works too"
                     ls -lah
                 '''
             }
         }
         stage('Git checkout') {
            steps {
                git branch: 'uploads3', credentialsId: '	github-credentials', url: 'https://github.com/antoniocivc/projeto-padawan'
            }
        }      
         stage('Upload to AWS') {
              steps {
                  withAWS(region:'us-east-1',credentials:'aws-credentials') {
                  sh 'echo "Uploading content with AWS creds"'
                      s3Upload(pathStyleAccessEnabled: true, payloadSigningEnabled: true, file:'index.html', bucket:'patomagico.com.br')
                  }
              }
         }
     }
}